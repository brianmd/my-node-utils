{
  "framework": "mocha",
  "src_files": [
    "node_modules/chai/chai.js",
    "app/classes/*",
    "test/setup.js",
    "test/*.test.js",
    "index.js"
  ],
  "launchers": {
    "node": {
      "command": "./node_modules/.bin/mocha -r test/setup.js -R tap test/*.test.js",
      "protocol": "tap"
    }
  },
  "launch_in_dev": [
    "node"
  ],
  "url": "http://localhost:9876/",
  "phantomjs_debug_port": 9877,
  "port": null
}

